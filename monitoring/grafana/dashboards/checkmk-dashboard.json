{
  "dashboard": {
    "title": "CheckMK Monitoring - Ensurance Pharmacy",
    "tags": ["checkmk", "monitoring", "ensurance"],
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Host Status",
        "type": "stat",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "checkmk_host_up",
            "legendFormat": "{{host}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 6, "w": 24, "x": 0, "y": 0},
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "orientation": "horizontal"
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 1, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "CPU Usage (%)",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "checkmk_cpu_usage_percent",
            "legendFormat": "{{host}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [70], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "CheckMK CPU Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 3,
        "title": "Memory Usage (%)",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "checkmk_memory_usage_percent",
            "legendFormat": "{{host}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [80], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "CheckMK Memory Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 4,
        "title": "Disk Usage (%)",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "checkmk_disk_usage_percent",
            "legendFormat": "{{host}} - {{mount}}",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "yaxes": [
          {"format": "percent", "min": 0, "max": 100},
          {"format": "short"}
        ],
        "alert": {
          "conditions": [
            {
              "evaluator": {"params": [75], "type": "gt"},
              "operator": {"type": "and"},
              "query": {"params": ["A", "5m", "now"]},
              "reducer": {"params": [], "type": "avg"},
              "type": "query"
            }
          ],
          "executionErrorState": "alerting",
          "frequency": "1m",
          "handler": 1,
          "name": "CheckMK Disk Alert",
          "noDataState": "no_data",
          "notifications": []
        }
      },
      {
        "id": 5,
        "title": "Network Traffic (bytes/s)",
        "type": "graph",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "rate(checkmk_network_in_bytes[5m])",
            "legendFormat": "{{host}} - {{interface}} IN",
            "refId": "A"
          },
          {
            "expr": "rate(checkmk_network_out_bytes[5m])",
            "legendFormat": "{{host}} - {{interface}} OUT",
            "refId": "B"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "yaxes": [
          {"format": "Bps"},
          {"format": "short"}
        ],
        "seriesOverrides": [
          {
            "alias": "/OUT/",
            "transform": "negative-Y"
          }
        ]
      },
      {
        "id": 6,
        "title": "Service States",
        "type": "table",
        "datasource": "Prometheus",
        "targets": [
          {
            "expr": "checkmk_service_state",
            "format": "table",
            "instant": true,
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 22},
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {},
              "renameByName": {
                "host": "Host",
                "service": "Service",
                "Value": "State"
              }
            }
          }
        ],
        "options": {
          "showHeader": true
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "left",
              "displayMode": "color-background"
            },
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1, "color": "yellow"},
                {"value": 2, "color": "red"}
              ]
            }
          }
        }
      }
    ],
    "refresh": "30s",
    "schemaVersion": 27,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  },
  "overwrite": false
}
