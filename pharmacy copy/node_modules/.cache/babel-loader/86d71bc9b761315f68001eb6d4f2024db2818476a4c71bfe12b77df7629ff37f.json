{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport Comentarios from '@/components/Comentarios.vue';\nimport axios from \"axios\";\nconst ip = process.env.VUE_APP_IP;\nconst products = ref([]);\n// 2. Define tus productos utilizando las imágenes importadas\nconst fetchProduct = async () => {\n  try {\n    const response = await axios.get(`http://${ip}:8081/api2/medicines`);\n    products.value = response.data;\n    console.log(products.value);\n  } catch (error) {\n    console.error('Error fetching medicines:', error);\n  }\n};\nfetchProduct();\nexport default {\n  name: \"ProductoDetalle\",\n  components: {\n    Comentarios\n  },\n  // Permite que el ID llegue como prop (asegúrate de que en el router usas props: true)\n  props: [\"id\"],\n  data() {\n    return {\n      product: null,\n      productComments: [{\n        id: 1,\n        author: 'Juan',\n        text: 'Muy buen producto!'\n      }, {\n        id: 2,\n        author: 'Maria',\n        text: 'Me ayudó mucho, gracias!'\n      }]\n    };\n  },\n  mounted() {\n    // Ejemplo local: array de productos “mock”\n    const productos = [{\n      id: '1',\n      name: 'Botellas Médicas',\n      price: '$35.00',\n      images: [require('@/assets/botellas.jpg'), require('@/assets/medicina.png')],\n      description: 'Detalle de Botellas Médicas',\n      inventory: 100,\n      activeIngredient: 'Acetaminofén',\n      generalDescription: 'Producto para uso médico, etc.',\n      usageRecommendation: 'Recomendación de uso...'\n    }, {\n      id: '2',\n      name: 'Farmacia General',\n      price: '$25.00',\n      images: [require('@/assets/farmacia.png'), require('@/assets/mas.jpeg')],\n      description: 'Detalle de Farmacia General',\n      inventory: 50,\n      activeIngredient: 'Ibuprofeno',\n      generalDescription: 'Preparado para uso general en farmacias.',\n      usageRecommendation: 'Recomendado para el alivio del dolor.'\n    }\n    // Puedes agregar más productos según lo necesites\n    ];\n\n    // Se filtra el producto que coincida con el ID recibido en la ruta\n    this.product = productos.find(prod => prod.id === '1'); // Mostrar el detalle del producto 1\n  }\n};","map":{"version":3,"names":["Comentarios","axios","ip","process","env","VUE_APP_IP","products","ref","fetchProduct","response","get","value","data","console","log","error","name","components","props","product","productComments","id","author","text","mounted","productos","price","images","require","description","inventory","activeIngredient","generalDescription","usageRecommendation","find","prod"],"sources":["/Users/nohelestradap/Documents/VsCode/ensurancePharmacy/pharmacy/src/pages/ProductoDetalle.vue"],"sourcesContent":["<!-- eslint-disable vue/multi-word-component-names -->\n<template>\n    <div>\n      <h1>Detalle del Producto</h1>\n  \n      <!-- Si el producto existe, mostramos su información -->\n      <div v-if=\"product\">\n        <h2>{{ product.name }}</h2>\n        <!-- Mostrar imagen principal si existe -->\n        <img \n          v-if=\"product.images && product.images.length\" \n          :src=\"product.images[0]\" \n          alt=\"Imagen principal del producto\" \n          style=\"width: 200px; border-radius: 8px;\"\n        />\n  \n        <p><strong>Precio:</strong> {{ product.price }}</p>\n        <p><strong>Descripción:</strong> {{ product.description }}</p>\n        <p><strong>Inventario:</strong> {{ product.inventory }}</p>\n        <p><strong>Ingrediente Activo:</strong> {{ product.activeIngredient }}</p>\n        <p><strong>Descripción General:</strong> {{ product.generalDescription }}</p>\n        <p><strong>Recomendación de Uso:</strong> {{ product.usageRecommendation }}</p>\n        <Comentarios :initialComments=\"productComments\" />\n      </div>\n      <!-- Si no existe el producto, mostramos un mensaje de error -->\n      <div v-else>\n        <p>Producto no encontrado.</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  import Comentarios from '@/components/Comentarios.vue';\n  import axios from \"axios\";\n  const ip = process.env.VUE_APP_IP;\n\n  const products = ref([]);\n  // 2. Define tus productos utilizando las imágenes importadas\n  const fetchProduct = async () => {\n    try {\n      const response = await axios.get(`http://${ip}:8081/api2/medicines`);\n      products.value = response.data;\n      console.log(products.value);\n    } catch (error) {\n      console.error('Error fetching medicines:', error);\n    }\n  };\n  fetchProduct();\n\n  export default {\n    name: \"ProductoDetalle\",\n    components: {\n      Comentarios\n    },\n    // Permite que el ID llegue como prop (asegúrate de que en el router usas props: true)\n    props: [\"id\"],\n    \n    data() {\n      return {\n        product: null,\n        productComments: [\n          { id: 1, author: 'Juan', text: 'Muy buen producto!' },\n          { id: 2, author: 'Maria', text: 'Me ayudó mucho, gracias!' }\n        ]\n      };\n    },\n    \n    mounted() {\n      // Ejemplo local: array de productos “mock”\n      const productos = [\n        {\n          id: '1',\n          name: 'Botellas Médicas',\n          price: '$35.00',\n          images: [\n            require('@/assets/botellas.jpg'),\n            require('@/assets/medicina.png')\n          ],\n          description: 'Detalle de Botellas Médicas',\n          inventory: 100,\n          activeIngredient: 'Acetaminofén',\n          generalDescription: 'Producto para uso médico, etc.',\n          usageRecommendation: 'Recomendación de uso...'\n        },\n        {\n          id: '2',\n          name: 'Farmacia General',\n          price: '$25.00',\n          images: [\n            require('@/assets/farmacia.png'),\n            require('@/assets/mas.jpeg')\n          ],\n          description: 'Detalle de Farmacia General',\n          inventory: 50,\n          activeIngredient: 'Ibuprofeno',\n          generalDescription: 'Preparado para uso general en farmacias.',\n          usageRecommendation: 'Recomendado para el alivio del dolor.'\n        }\n        // Puedes agregar más productos según lo necesites\n      ];\n    \n      // Se filtra el producto que coincida con el ID recibido en la ruta\n      this.product = productos.find(prod => prod.id === '1'); // Mostrar el detalle del producto 1\n    }\n  };\n  </script>\n  \n  <style scoped>\n  /* Estilos opcionales */\n  </style>"],"mappings":";;AAgCE,OAAOA,WAAU,MAAO,8BAA8B;AACtD,OAAOC,KAAI,MAAO,OAAO;AACzB,MAAMC,EAAC,GAAIC,OAAO,CAACC,GAAG,CAACC,UAAU;AAEjC,MAAMC,QAAO,GAAIC,GAAG,CAAC,EAAE,CAAC;AACxB;AACA,MAAMC,YAAW,GAAI,MAAAA,CAAA,KAAY;EAC/B,IAAI;IACF,MAAMC,QAAO,GAAI,MAAMR,KAAK,CAACS,GAAG,CAAC,UAAUR,EAAE,sBAAsB,CAAC;IACpEI,QAAQ,CAACK,KAAI,GAAIF,QAAQ,CAACG,IAAI;IAC9BC,OAAO,CAACC,GAAG,CAACR,QAAQ,CAACK,KAAK,CAAC;EAC7B,EAAE,OAAOI,KAAK,EAAE;IACdF,OAAO,CAACE,KAAK,CAAC,2BAA2B,EAAEA,KAAK,CAAC;EACnD;AACF,CAAC;AACDP,YAAY,CAAC,CAAC;AAEd,eAAe;EACbQ,IAAI,EAAE,iBAAiB;EACvBC,UAAU,EAAE;IACVjB;EACF,CAAC;EACD;EACAkB,KAAK,EAAE,CAAC,IAAI,CAAC;EAEbN,IAAIA,CAAA,EAAG;IACL,OAAO;MACLO,OAAO,EAAE,IAAI;MACbC,eAAe,EAAE,CACf;QAAEC,EAAE,EAAE,CAAC;QAAEC,MAAM,EAAE,MAAM;QAAEC,IAAI,EAAE;MAAqB,CAAC,EACrD;QAAEF,EAAE,EAAE,CAAC;QAAEC,MAAM,EAAE,OAAO;QAAEC,IAAI,EAAE;MAA2B;IAE/D,CAAC;EACH,CAAC;EAEDC,OAAOA,CAAA,EAAG;IACR;IACA,MAAMC,SAAQ,GAAI,CAChB;MACEJ,EAAE,EAAE,GAAG;MACPL,IAAI,EAAE,kBAAkB;MACxBU,KAAK,EAAE,QAAQ;MACfC,MAAM,EAAE,CACNC,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,uBAAuB,EAChC;MACDC,WAAW,EAAE,6BAA6B;MAC1CC,SAAS,EAAE,GAAG;MACdC,gBAAgB,EAAE,cAAc;MAChCC,kBAAkB,EAAE,gCAAgC;MACpDC,mBAAmB,EAAE;IACvB,CAAC,EACD;MACEZ,EAAE,EAAE,GAAG;MACPL,IAAI,EAAE,kBAAkB;MACxBU,KAAK,EAAE,QAAQ;MACfC,MAAM,EAAE,CACNC,OAAO,CAAC,uBAAuB,CAAC,EAChCA,OAAO,CAAC,mBAAmB,EAC5B;MACDC,WAAW,EAAE,6BAA6B;MAC1CC,SAAS,EAAE,EAAE;MACbC,gBAAgB,EAAE,YAAY;MAC9BC,kBAAkB,EAAE,0CAA0C;MAC9DC,mBAAmB,EAAE;IACvB;IACA;IAAA,CACD;;IAED;IACA,IAAI,CAACd,OAAM,GAAIM,SAAS,CAACS,IAAI,CAACC,IAAG,IAAKA,IAAI,CAACd,EAAC,KAAM,